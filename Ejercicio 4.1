Algoritmo mayorDeCuatro
	
	numMayor <- 0;
	
	Escribir "Ingrese 4 números: ";
	
	Leer num1;
	Si (num1 > numMayor) Entonces
		numMayor <- num1;
	FinSi
	
	Leer num2;
	Si (num2 > numMayor) Entonces
		numMayor <- num2;
	FinSi
	
	Leer num3;
	Si (num3 > numMayor) Entonces
		numMayor <- num3;
	FinSi
	
	Leer num4;
	Si (num4 > numMayor) Entonces
		numMayor <- num4;
	FinSi
	
	Escribir "El numero mayor es: ", numMayor;
	
FinAlgoritmo
